    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "start:prod": "cross-env NODE_ENV=production node scripts/start.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o docs/storybook",
    "storybook:build-static": "build-storybook -c .storybook -o .storybook-static",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/addon-viewport": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "@storybook/storybook-deployer": "^2.3.0",
